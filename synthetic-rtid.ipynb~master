    "treatment_costs = {\n",
    "    \"No discount\": 0,\n",
    "    \"5% off\": 0.5,\n",
    "    \"Coupon\": 1.2, \n",
    "}\n",
    "\n",
    "r = KArm(cd, treatment_costs)\n"
   ]
